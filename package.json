{
  "name": "indecks",
  "version": "2.0.0",
  "description": "An interactive index card built in ReactJS",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/danyim/indecks.git"
  },
  "author": "Daniel Yim",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn test -- --coverage",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "stylefmt",
      "stylelint",
      "git add"
    ],
    "*.styl": [
      "stylefmt",
      "stylint",
      "git add"
    ]
  },
  "dependencies": {
    "@attently/riek": "^2.0.1",
    "autoprefixer": "7.1.2",
    "axios": "^0.18.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.6",
    "comma-separated-values": "^3.6.4",
    "connected-react-router": "^4.3.0",
    "css-loader": "0.28.4",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "firebase": "^5.2.0",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "jest-styled-components": "^5.0.1",
    "json2csv": "^4.1.5",
    "lint-staged": "^7.2.0",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.13.7",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.12",
    "react-inline-edit": "^3.0.0",
    "react-keybinding-component": "^0.5.1",
    "react-modal": "^3.4.5",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-style-proptype": "^3.2.1",
    "react-swipeable": "^4.3.0",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "remarkable": "^1.7.1",
    "reselect": "^3.0.1",
    "slug": "^0.9.1",
    "style-loader": "0.19.0",
    "styled-components": "^3.3.3",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/window.js",
      "<rootDir>/config/enzyme/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "\\.(css|styl)": "identity-obj-proxy",
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "flow",
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true,
      "jasmine": true
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.79.1",
    "identity-obj-proxy": "^3.0.0"
  }
}
