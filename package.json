{
  "name": "indecks",
  "version": "1.0.0",
  "description": "An interactive index card built in ReactJS",
  "scripts": {
    "prepush": "npm run lint",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "jest --config ./jest.json",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:nyc": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "clean": "rimraf dist",
    "start": "node devServer.js",
    "publish:git": "npm run build && mkdir dist/dist && mv dist/*.* dist/dist && cp index.html dist/index.html && sed -e 's/\\/dist/dist/g' dist/index.html > dist/index.html.tmp && mv dist/index.html.tmp dist/index.html && gh-pages -d dist",
    "tunnel": "browser-sync start --proxy localhost:7770",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint -c .eslintrc src --ext .jsx",
    "lint:styles": "stylint src/styles/**/*.styl",
    "watch:lint": "npm-run-all --parallel lint:* -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danyim/indecks.git"
  },
  "author": "Daniel Yim",
  "license": "MIT",
  "homepage": "https://github.com/danyim/indecks",
  "engines": {
    "node": ">=7.2.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "enzyme": "^2.8.2",
    "eslint-plugin-jsx": "^0.0.2",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "immutable": "^3.8.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "re-base": "^2.7.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-dropzone": "3.12.4",
    "react-inline-edit": "^3.0.0",
    "react-keybinding-component": "^0.4.1",
    "react-modal": "^1.7.6",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-transitions": "^2.2.1",
    "react-swipeable": "^3.9.2",
    "react-test-renderer": "^15.5.4",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.6",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.1",
    "redux-persist": "^4.6.0",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "reselect": "^3.0.0",
    "riek": "^1.0.7",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-display-name": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.20.2",
    "expect-jsx": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "gh-pages": "^0.12.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "postcss-loader": "^1.3.3",
    "poststylus": "^0.2.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  }
}
