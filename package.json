{
  "name": "indecks",
  "version": "1.0.0",
  "description": "An interactive index card built in ReactJS",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "jest --config ./jest.json",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:nyc": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "clean": "rimraf dist",
    "start": "node devServer.js",
    "publish:git": "npm run build && mkdir dist/dist && mv dist/*.* dist/dist && cp index.html dist/index.html && sed -e 's/\\/dist/dist/g' dist/index.html > dist/index.html.tmp && mv dist/index.html.tmp dist/index.html && gh-pages -d dist",
    "tunnel": "browser-sync start --proxy localhost:7770",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint src/**/*.jsx --fix && prettier --no-semi --single-quote --write src/**/*.jsx",
    "lint:styles": "stylint src/styles/**/*.styl",
    "watch:lint": "npm-run-all --parallel lint:* -- --watch"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write --no-semi --single-quote",
      "git add"
    ],
    "*.css": [
      "stylefmt",
      "stylelint",
      "git add"
    ],
    "*.styl": [
      "stylefmt",
      "stylint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danyim/indecks.git"
  },
  "author": "Daniel Yim",
  "license": "MIT",
  "homepage": "https://github.com/danyim/indecks",
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "comma-separated-values": "^3.6.4",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint-plugin-jsx": "^0.0.2",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.11.2",
    "firebase": "^4.3.0",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "json2csv": "^3.11.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "re-base": "^3.0.3",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dnd": "^2.4.0",
    "react-dom": "^15.6.1",
    "react-dropzone": "^3.13.3",
    "react-inline-edit": "^3.0.0",
    "react-keybinding-component": "^0.5.1",
    "react-modal": "^2.2.4",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-style-proptype": "^3.0.0",
    "react-swipeable": "^4.1.0",
    "react-test-renderer": "^15.6.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.9.1",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.6.2",
    "reselect": "^3.0.1",
    "riek": "^1.1.0",
    "slug": "^0.9.1",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-import-extension-jsx-to-js": "^0.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-pretty-standard": "^0.2.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^1.18.0",
    "expect-jsx": "^3.0.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-styled-components": "^3.3.2",
    "lint-staged": "^4.0.4",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.1.0",
    "postcss-loader": "^2.0.6",
    "poststylus": "^0.2.3",
    "prettier": "^1.6.1",
    "rimraf": "^2.5.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "style-loader": "^0.18.2",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "$"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true,
      "jasmine": true,
      "jquery": true
    }
  }
}
