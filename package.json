{
  "name": "indecks",
  "version": "1.0.0",
  "description": "An interactive index card built in ReactJS",
  "scripts": {
    "prepush": "npm run lint",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:nyc": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "clean": "rimraf dist",
    "start": "node devServer.js",
    "publish:git": "npm run build && mkdir dist/dist && mv dist/*.* dist/dist && cp index.html dist/index.html && sed -e 's/\\/dist/dist/g' dist/index.html > dist/index.html.tmp && mv dist/index.html.tmp dist/index.html && gh-pages -d dist",
    "tunnel": "browser-sync start --proxy localhost:7770",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint -c .eslintrc src --ext .jsx",
    "lint:styles": "stylint src/styles/**/*.styl",
    "watch:lint": "npm-run-all --parallel lint:* -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danyim/indecks.git"
  },
  "author": "Daniel Yim",
  "license": "MIT",
  "homepage": "https://github.com/danyim/indecks",
  "dependencies": {
    "classnames": "^2.2.5",
    "enzyme": "^2.7.1",
    "eslint-plugin-jsx": "^0.0.2",
    "express": "^4.13.4",
    "file-loader": "^0.10.0",
    "immutable": "^3.8.1",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.2",
    "react-dropzone": "^3.5.3",
    "react-inline-edit": "^3.0.0",
    "react-keybinding-component": "^0.4.1",
    "react-modal": "^1.5.2",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.3",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-router-transitions": "^2.0.0",
    "react-swipeable": "^3.5.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "redux": "^3.5.2",
    "redux-form": "^6.0.2",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.8.1",
    "redux-persist": "^4.4.2",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.6.2",
    "reselect": "^2.5.3",
    "riek": "^1.0.7",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.18.0",
    "expect-jsx": "^3.0.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.5-beta.2",
    "gh-pages": "^0.12.0",
    "husky": "^0.13.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "poststylus": "^0.2.3",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|styl)$": "identity-obj-proxy"
    }
  }
}
